        <activity android:name=".presentation.ServicoDetailActivity" />
        <activity android:name=".presentation.HomeActivity" />
        <activity android:name=".presentation.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
